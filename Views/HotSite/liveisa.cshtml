@{
    ViewBag.Title = "Isa Domingues";
    Layout = "~/Views/Shared/Base/_Layout.cshtml";
}

<div class="ui container margin top medium">
        <div class="ui two column grid stackable middle aligned">
            <div class="row">
                <div class="column">
                    <h1>@ViewBag.Title</h1>
                </div>
            </div>
        </div>
    </div>

    <div class="ui container margin bottom large">
        <div class="ui two column grid stackable middle aligned margin bottom large">
            <div class="row one column">
                <div class="column">
                    <div class="products-carousel-mais">
                        @Html.Action("ProductsByGroup", "Product", new { id = 5081 })
                    </div>
                </div>
            </div>
        </div>
    </div>

    <script>
        $(document).ready(function () {            
            $('.products-carousel-mais .column > div').removeClass('slideshow');            
        });
    </script>