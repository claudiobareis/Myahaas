<!-- ~/Views/Shared/Blocks/_CentralHalfBanner.cshtml -->
@Html.Action("CentralHalfBanner", "Banner")

<section class="ui container category-2">
    <div class="title">
        <h2>mais vendidos</h2>
        <h3>os best sellers da mya reunidos aqui</h3>
    </div>
    <div class="products-carousel-mais">
        <!-- Chamada de exibição de um grupo  -->        
        @Html.Action("ProductsByGroup", "Product", new { id = 4940 })
    </div>
    <div class="category-cta">
        <a href="grupo/best-sellers/">VER TODOS</a>
    </div>
    <script>
        $(document).ready(function () {            
            $('.products-carousel-mais .column > div').removeClass('slideshow');            
        });
    </script>
</section>

<section class="ui container category-3">
    <div class="title">
        <h2 class="bee"><span class="bee">linha</span> home</h2>
        <h3>Mya Bee Home: coisinhas incríveis para tornar a sua casa o seu lugar favorito</h3>
    </div>
    <div class="products-carousel">
        <!-- Chamada de exibição de uma categoria  -->
        @Html.Action("GetProducts", "Product", new { category = 128952, pageSize = 8 })
    </div>
    <div class="category-cta">
        <a href="/categoria/mya-bee-home">VER TODOS</a>
    </div>
    <script>
        $(document).ready(function () {            
            $('.category-3 .cards').addClass('slideshow');
            $('.category-3 .slideshow').attr('data-dots','true');
            $('.category-3 .slideshow').attr('data-arrow','true');  
            $('.category-3 .product-in-card a.ui.attached').removeClass('slide');          
        });
    </script>   
</section>

