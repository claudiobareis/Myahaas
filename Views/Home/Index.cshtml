@{
    ViewBag.Title = string.IsNullOrEmpty(LojaCorp.Control.Company._Company.Title) ? LojaCorp.Control.Company._Company.NameStore : LojaCorp.Control.Company._Company.Title;
    Layout = "~/Views/Shared/Base/_Layout.cshtml";
}
<!-- ~/Views/Shared/Blocks/_Owl.cshtml -->
@Html.Partial("Blocks/_Owl")

<!-- ~/Views/Shared/Blocks/_InfoBar.cshtml -->
@Html.Partial("Blocks/_InfoBar")

<section class="mobile-hide tablet-hide hero-banner">
    <!-- ~/Views/Shared/Blocks/Banner/_FullBanner.cshtml -->
    @Html.Action("FullBanner", "Banner")
    <script>
        $(document).ready(function () {            
            $('.products-carousel .slideshow').attr('data-dots','true');
            $('.products-carousel .slideshow').attr('data-arrow','false');            
        });
    </script> 
</section>

<section class="tablet-and-mobile-only mobile-banner">
    <!-- ~/Views/Shared/Blocks/Banner/_FooterHalfBanner.cshtml -->
    @Html.Action("FooterHalfBanner", "Banner")
</section>

<section class="ui container category-1">
    <div class="title">
        <h2>Novidades da Semana</h2>
        <h3>Lançamento da tendência toda semana para você se apaixonar </h3>
    </div>
    <div class="products-carousel">
        <!-- Chamada de exibição de uma categoria  -->        
        @Html.Action("ProductsByGroup", "Product", new { id = 4865 })      
    </div>
    <div class="category-cta">
        <a href="/grupo/lancamentos/">VER TODOS</a>
    </div>
    <script>
        $(document).ready(function () {  
            $('.products-carousel .slideshow').attr('data-dots','true');
            $('.products-carousel .slideshow').attr('data-arrow','true'); 
            $('.category-1 .product-in-card a.ui.attached').removeClass('slide');  
          
