                                    <a class="text color white" href="@redesSociais.Find(f => f.Identifier == "facebook").Content" target="_blank">
                                        <i class="facebook icon"></i>
                                    </a>
                                }
                                @if (redesSociais.Find(f => f.Identifier == "instagram") != null)
