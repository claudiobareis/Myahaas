                                    <a class="text color white" href="@redesSociais.Find(f => f.Identifier == "instagram").Content" target="_blank">
                                        <i class="instagram icon"></i>
                                    </a>
                                }
                                @if (redesSociais.Find(f => f.Identifier == "twitter") != null)
