                            <span class="text">
                                Cadastre seu email e ganhe 10% na primeira compra
                            </span>
                        </div>
                        <div class="inline fields margin none">
                            <div class="field">
                                <div class="ui input">
                                    <input type="text" placeholder="seu@email.com.br" id="email_news">
                                </div>
                            </div>
                            <div class="field">
                                <button class="ui button red" id="btn_news">
                                    Ok
                                </button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

